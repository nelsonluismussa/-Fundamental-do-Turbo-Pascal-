{

    Operadores e Expressões:  
	Aritméticos: + / *- mod, 
	Relacionais: = <> > < >=  <= , 
	Lógicos : and, or, not.

}

Program  calcular;

var 
   a:integer;







   
   b:integer;
   soma: integer;

begin 
    writeln('a:');
    readln(a);
    writeln('b:');
    readln(b);
    soma:= a + b;
    writeln('soma:', soma);      
End.